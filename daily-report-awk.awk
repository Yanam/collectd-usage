#!/bin/awk -f{        if(NR==1) {                rowNum = NF;                if(rowNum ==1){title1=$1}                if(rowNum ==2) {title1=$1;title2=$2;}        }else {                if(rowNum ==1){                        if($2 !~ /-nan/){                                value1+=$2;sum1++;                        }                }else if(rowNum ==2){                        if($2 !~ /-nan/) {                                value1+=$2;sum1++;                        }                        if($3 !~ /-nan/) {                                value2+=$3;sum2++;                        }                }                                }}END{        if(NF ==2) {                print ENVIRON["filename"]" "ENVIRON["type"]" "title1":"value1/sum1        } else if (NF ==3) {                print ENVIRON["filename"]" "ENVIRON["type"]" "title1":"value1/sum1";"title2":"value2/sum2        }}